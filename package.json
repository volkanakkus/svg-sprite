{
    "name": "@volkanakkus/nuxt-svg-sprite",
    "version": "1.0.3",
    "description": "Easy way to use and optimize svg files in Nuxt.js",
    "repository": "nuxt-community/svg-sprite-module",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
          "types": "./dist/types.d.ts",
          "import": "./dist/module.mjs",
          "require": "./dist/module.cjs"
        }
      },
      "main": "./dist/module.cjs",
      "types": "./dist/types.d.ts",
      "files": [
        "dist"
      ],
    "scripts": {
        "prepack": "yarn nuxt-module-build build",
        "dev": "nuxi dev playground",
        "dev:build": "yarn nuxi build playground",
        "dev:prepare": "yarn nuxt-module-build build --stub && yarn nuxt-module-build prepare && yarn nuxi prepare playground",
        "dev:docs": "cd docs && yarn && yarn dev",
        "codesandbox": "cd .codesandbox && yarn && yarn dev",
        "release": "npm run lint && npm run prepack && changelogen && npm publish && git push --follow-tags",
        "lint": "eslint .",
        "test": "vitest run",
        "test:watch": "vitest watch"
    },
    "dependencies": {
        "@nuxt/kit": "^3.10.3",
        "svgo": "^3.2.0"
    },
    "devDependencies": {
        "@nuxt/module-builder": "^0.5.5",
        "@nuxt/test-utils": "^3.11.0",
        "@nuxtjs/eslint-config-typescript": "latest",
        "@types/svgo": "^2.6.4",
        "@nuxt/devtools": "latest",
        "@nuxt/eslint-config": "^0.2.0",
        "playwright": "^1.41.2",
        "release-it": "^17.6.0",
        "@nuxt/schema": "^3.10.3",
        "@types/node": "^20.11.22",
        "eslint": "^8.57.0",
        "nuxt": "^3.10.3",
        "vitest": "^1.3.1"
    },
    "resolutions": {
        "string-width": "4.2.3"
    }
}
